{
  "compilerOptions": {
    "module": "commonjs",
    "esModuleInterop": true,
    "target": "es6",
    "strict": true,
    "moduleResolution": "node",
    "sourceMap": true,
    "outDir": "dist",
    "baseUrl": ".",
    "jsx": "react-jsx",
    "allowJs": true,
    "skipLibCheck": true,
    "paths": {
      "@rese/*": ["packages/*/tsconfig.json"]
    },
    
  },
  "include": ["./packages/server/**/*", "./packages/client/**/*"]
}
