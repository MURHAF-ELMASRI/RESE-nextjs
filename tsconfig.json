{
  "compilerOptions": {
    "module": "commonjs",
    "esModuleInterop": true,
    "target": "es6",
    "strict": true,
    "moduleResolution": "node",
    "sourceMap": true,
    "outDir": "dist",
    "baseUrl": ".",
    "allowJs": true,
    "skipLibCheck": true,
    "checkJs": true,
    "importHelpers": true,
    "paths": {
      "@rese/*": ["packages/*"]
    }
  },
  "include": ["./packages/server/**/*"]
}
